<%= simple_form_for @chalet do |f| %>
  <%= f.input :name %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :number_of_guests %>
  <%= f.input :number_of_bedrooms %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true }, label: "Photos" %>
  <%= f.submit label: "Add Your Chalet" %>
<% end %>
